<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wannengyongyao.drug.dao.DrugOrderImageMapper">
  <resultMap id="OrderImage" type="com.wannengyongyao.drug.model.DrugOrderImage">
    <id column="id" jdbcType="BIGINT" property="id" />
    <result column="order_id" jdbcType="BIGINT" property="orderId" />
    <result column="image_url" jdbcType="VARCHAR" property="imageUrl" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
  </resultMap>
  <sql id="Base_Column_List">
    id, order_id, image_url, create_time
  </sql>
  <select id="get" parameterType="java.lang.Long" resultMap="OrderImage">
    select 
    <include refid="Base_Column_List" />
    from drug_order_image
    where id = #{id,jdbcType=BIGINT}
  </select>

  <insert id="insert" parameterType="com.wannengyongyao.drug.model.DrugOrderImage">
    insert into drug_order_image (
      id,
      order_id,
      image_url,
      create_time)
    values (
      #{id,jdbcType=BIGINT},
      #{orderId,jdbcType=BIGINT},
      #{imageUrl,jdbcType=VARCHAR},
      #{createTime,jdbcType=TIMESTAMP})
  </insert>

</mapper>