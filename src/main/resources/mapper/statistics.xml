<mapper namespace="com.blueocean.azbrain.dao.StatisticsMapper">
  <select id="userQuestionRank" resultType="java.util.HashMap">
    SELECT
      u.id,COUNT(question_id)
    FROM
      azbrain_question q
    INNER JION
      azbrain_user u
    ON
      q.user_id = u.id
    WHERE

    GROUP BY

    ORDER BY

  </select>

</mapper>